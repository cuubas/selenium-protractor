  "version": "2.0.0",
  "description": "Selenium test case converter to protractor source code",
  "repository": {
    "type": "git",
    "url": "https://github.com/cuubas/selenium-protractor"
  },
  "license": "MIT",
  "keywords": [
    "selenium",
